{
  "expo": {
    "name": "AICaddy Pro",
    "slug": "aicaddypro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aicaddypro",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tfunk1030.aicaddypro",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "AICaddy Pro uses your location to get local weather conditions for accurate shot calculations.",
        "NSMotionUsageDescription": "AICaddy Pro uses your device compass to detect the direction you're aiming at for wind-adjusted shot calculations.",
        "UIBackgroundModes": [],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "AICaddy Pro uses your camera for the HUD wind overlay \u2014 point at the flag to see wind-adjusted data over a live view. No photos or video are saved."
      }
    },
    "android": {
      "package": "com.tfunk1030.aicaddypro",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0A0A0A"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22d97181-5980-4d2c-ab9f-58036e334d0d"
      }
    },
    "owner": "tfunk1030"
  }
}